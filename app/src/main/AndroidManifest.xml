<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.norto.trabalhofrete">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnderecoActivity"
            android:label="@string/title_activity_estado"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadastrosActivity"
            android:label="@string/title_activity_cadastros"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadValorFreteActivity"
            android:label="@string/title_activity_cad_valor_frete"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>